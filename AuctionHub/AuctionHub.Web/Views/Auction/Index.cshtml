@model IEnumerable<IndexAuctionViewModel>

@{
    ViewBag.Title = "Index";

}

<div class="row">
    <div class="col s3"></div>
    <div class="col s6">
        @foreach (var auction in Model)
        {
            <div class="col s4">
                <h3><a asp-action="Details" asp-route-id="@auction.Id">@auction.ProductName</a></h3>
            </div>
        }
    </div>
    <div class="col s3"></div>

</div>
